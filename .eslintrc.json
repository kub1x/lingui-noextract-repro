{
  "env": {
    "node": true,
      "browser": true,
      "es2020": true
  },
    "extends": [
      "plugin:react/recommended",
    "airbnb"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 11,
      "sourceType": "module"
    },
    "rules": {
      "quotes": ["error", "single"],
      "jsx-quotes": ["error", "prefer-double"],
    }
}
